<Window x:Class="WorkTask.View.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkTask.View"
        mc:Ignorable="d"
        Title="ReportWindow" Height="450" Width="800" ResizeMode="NoResize">
    <Grid x:Name="FirstDatePicker">
        <DataGrid x:Name="SupplyDataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,32,447,0"/>
        <DatePicker HorizontalAlignment="Left" Margin="39,5,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="SecondDatePicker" HorizontalAlignment="Left" Margin="183,5,0,0" VerticalAlignment="Top"/>
        <Label Content="От" HorizontalAlignment="Left" Margin="10,4,0,0" VerticalAlignment="Top" Height="26"/>
        <Label Content="До" HorizontalAlignment="Left" Margin="154,4,0,0" VerticalAlignment="Top" Height="26"/>
        <DataGrid x:Name="SupplyGoodsDataGrid" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="447,32,10,0"/>

    </Grid>
</Window>
