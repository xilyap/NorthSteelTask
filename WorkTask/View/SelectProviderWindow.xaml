        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkTask.View"
        mc:Ignorable="d"
        Title="SelectProviderWindow" Height="238" Width="370" ResizeMode="NoResize" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="SubmitBtn" Content="Подтвердить" HorizontalAlignment="Center" Margin="0,163,0,0" VerticalAlignment="Top" Width="182" Height="31" Click="SubmitBtn_Click"/>
        <Label Content="Выберите поставщика" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="34" Width="218" FontSize="18" HorizontalContentAlignment="Center"/>
        <ComboBox x:Name="ProviderComboBox" HorizontalAlignment="Center" Margin="0,84,0,0" VerticalAlignment="Top" Width="182" Height="27"/>

    </Grid>
</Window>
