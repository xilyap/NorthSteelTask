<Window x:Class="WorkTask.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkTask.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="323" ResizeMode="NoResize">
    <Grid Margin="0,10,0,0">
        <Button x:Name="GetSupplyBtn" Content="Зарегистрировать поставку" HorizontalAlignment="Center" Width="276" Click="GetSupplyBtn_Click" Margin="0,71,0,0" Height="25" VerticalAlignment="Top"/>
        <Button x:Name="SupplyReport" Content="Отчёт по поставкам" HorizontalAlignment="Center" Margin="0,41,0,0" Width="276" Click="SupplyReportBtn_Click" Height="25" VerticalAlignment="Top"/>
        <Label Content="Интерфейс поставок" HorizontalAlignment="Center" VerticalAlignment="Top" Width="276" HorizontalContentAlignment="Center"/>

    </Grid>
</Window>
